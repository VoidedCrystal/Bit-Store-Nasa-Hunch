<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>File Download Page</title>
</head>
<body>
    <h1>Available Files</h1>
    <ul>
        <% files.forEach(function(file) { %>
            <li>
                <a href="/download/<%= file.filename %>"><%= file.originalname %></a>
            </li>
        <% }) %>
    </ul>
</body>
</html>
